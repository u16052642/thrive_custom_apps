<?xml version="1.0" encoding="utf-8"?>
<thrive>
    <!--To add HTML notes in delivery pdf report -->
    <template id="report_delivery_document"
              inherit_id="stock.report_delivery_document">
        <xpath expr="//div[@name='signature']" position="after">
            <p t-field="o.note"/>
        </xpath>
    </template>
</thrive>
